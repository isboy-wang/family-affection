/* 整体 */
#photograph{
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  width: 750rpx;
  height:1100rpx;
  align-items: center;
}
/* 编辑部分 */
.compile{
  display: flex;
  flex-direction: column;
  justify-content: space-evenly;
  align-items: center;
}
.photo{
  display: flex;
  align-items: center;
  margin-top: 50rpx;
  width: 650rpx;
}
.close{
  height: 40rpx;
  width: 40rpx;
  border-radius: 20rpx;
  background: rgb(224, 67, 67);
  font-size: 45rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  position: relative;
  right:-210rpx;
  top: -90rpx;


}
.photo image{
  height: 200rpx;
  width: 200rpx;
  border-radius: 6px;

}
/* 图片部分结束 */
/* 标签部分 */
.label{
  width: 650rpx;
  display: flex;
  justify-content: space-evenly;
  margin-top: 50rpx;

}
.label button{
  background: rgb(224, 148, 103);

  border-radius: 20px;
margin: 0px 20rpx;
}
.write{
  margin-top: 80rpx;
  margin-left: -50rpx;
  border-bottom:1px solid #535353;
  width: 500rpx;
  padding-bottom: 20rpx;
}
/* 编辑部分结束  */
/* 发布部分 */
#issue{
  width: 300rpx;
  height: 100rpx;
  background: rgb(226, 152, 100);
  display: flex;
  justify-content: center;
  align-items: center;
  border-radius: 5px;
  color: #ffffff;
  font-weight: 600;
  font-size: 20px;

}



